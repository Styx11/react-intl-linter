{
	"extends": "../tsconfig.base.json",
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2017",
		"outDir": "out",
		"rootDir": "src",
		"lib": [ "es2017" ],
		"sourceMap": true,
		"esModuleInterop": true,
		"resolveJsonModule": true,
	},
	"include": [
		"src"
	],
	"exclude": [
		"node_modules",
		"package-lock.json",
		"package.json"
	]
}